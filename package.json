{
  "name": "rainy-mood",
  "version": "0.0.1",
  "private": true,
  "author": "Anatol Marezhanyi @e1r0nd_crg",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0",
    "npm": "^3.0.0"
  },
  "browserslist": {
    "production": [
      "> 3%",
      "last 5 version",
      "ie 11"
    ],
    "development": [
      "last 1 version"
    ]
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prebuild": "npm install",
    "start": "set NODE_ENV=development&webpack-dev-server --inline --hot --open",
    "build": "set NODE_ENV=production&webpack --profile --display-modules --display-reasons --display-error-details",
    "lint": "eslint app/**/*.js",
    "test": "mocha --compilers js:babel-core/register",
    "coverage": "istanbul cover --include-all-sources node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register -R spec test/",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "snyk-protect": "snyk protect",
    "pre--publish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e1r0nd/rainy-mood.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-lsc-storage": "^0.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "2.24.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "npm": "^4.1.2",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "scss-loader": "0.0.1",
    "snyk": "^1.41.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.4"
  },
  "snyk": true,
  "dependencies": {}
}
